<configs>
	<append xpath="/twitch">
    <!-- Custom zombie tempate based on video -->
    <action name="twitch_CactusMan">
      <property name="command" value="#spawn_CactusMan" /> 
      <property name="category" value="Spawns" />
      <property name="event_name" value="spawn_CactusMan" />
      <property name="default_cost" value="200" />
      <property name="description_key" value="TwitchAction_SpawnCopDesc" />
      <property name="add_cooldown" value="true" />
      <property name="cooldown_blocked" value="true" />
      <property name="waiting_blocked" value="true" />
      <property name="cooldown" value="60" />
    </action>
    <!-- Template to make normal zombie spawn edits -->
    <action name="twitch_Demox3">
      <property name="command" value="#spawn_Demox3" />
      <property name="category" value="Spawns" />
      <property name="event_name" value="spawn_Demox3" />
      <property name="default_cost" value="100" />
      <property name="description_key" value="TwitchAction_SpawnDemoDesc" />
      <property name="add_cooldown" value="true" />
      <property name="cooldown_blocked" value="true" />
      <property name="waiting_blocked" value="true" />
      <property name="start_gamestage" value="0" />
      <property name="cooldown" value="60" />
      <property name="enabled" value="true" />
    </action>
	</append>
</configs>